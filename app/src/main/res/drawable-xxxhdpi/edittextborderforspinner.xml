<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <!-- solid background -->
            <solid android:color="@color/white"/>
            <corners
                android:radius="2dp"
                />


            <!-- draw a border around rectangle shape-->
            <stroke
                android:width="0.5dp"
                />
        </shape>
    </item>

</selector>
